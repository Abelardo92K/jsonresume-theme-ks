{{#resume.basics}}
<header class="resume-header clearfix">
  <div class="background-header {{#isDev label }} background-dev {{else}} background-employee {{/isDev}}">
    <div class="profile-pic pull-left">
      {{#if image}}
        <img src="{{image}}" alt="profile-pic">
      {{/if}}
    </div>
    <div class="profile-header pull-right">
      <div class="my-auto">
        <h1>{{name}}</h1>
        <h2>{{label}}</h2>
      </div>
    </div>
  </div>
</header>
{{/resume.basics}}
