{
  "basics": {
    "name": "{{basics.name}}",
    "label": "{{basics.label}}",
    "image": "{{basics.image}}",
    "url": "{{basics.url}}",
    "summaryPreview": "{{#basics.summary}}<div class='summary-container'><div class='title'><h3>Professional Summary</h3></div><p class='summary'>{{basics.summary}}</p></div>{{/basics.summary}}"
  },
  "work": "{{#work}}<h4 class='summary-position'>{{position}}</h4><h4>{{company}}</h4><h4>{{projectName}}</h4><h4>{{client}}</h4><h4>{{city}}.{{#startDate}}<span class='startDate'>{{startDate}}</span>{{/startDate}}{{#endDate}}<span class='endDate'> to {{endDate}}</span>{{/endDate}}</h4>{{#skills}}{{.}} |{{/skills}}{{#technologies}}{{.}} |{{/technologies}}{{/work}}",
  "education": "{{#education.length}}<div class='container education-container'><div class='title'><h3>Education</h3></div>{{#education}}<section class='item'>{{#studyType}}<h4>{{studyType}}</h4>{{/studyType}}{{#institution}}<h4>{{institution}}</h4>{{/institution}}<h4>({{#startDate}}<span class='startDate'>{{startDate}}</span>{{/startDate}}{{#endDate}}<span class='endDate'> - {{endDate}}</span>{{/endDate}}{{#releaseDate}}{{releaseDate}}{{/releaseDate}})</h4></section>{{/education}}{{/education.length}}",
  "awards": "{{#awards.length}}<div class='container awards-container'><div class='title'><h3>Awards</h3></div>{{#awards}}<h4>{{title}}</h4>{{/awards}}</div>{{/awards.length}}",
  "skills": "{{#skills}}<h4>{{name}}</h4>{{/skills}}",
  "languages": "{{#languages}}<h4>{{language}} {{#fluency}}: {{fluency}} {{/fluency}}</h4>{{/languages}}"
}